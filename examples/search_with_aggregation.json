{
    "query": "search",
    "boost": 1.0,
    "start": 0,
    "num": 5,
    "sort_by": "-_score",
    "fields": [
        "id",
        "title"
    ],
    "aggregations": {
        "text_terms": {
            "type": "terms",
            "options": {
                "field": "text",
                "min_length": 1,
                "max_length": 20,
                "size": 10
            }
        },
        "id_range": {
            "type": "range",
            "options": {
                "field": "id",
                "ranges": {
                    "low": {
                        "low": 0,
                        "high": 500
                    },
                    "medium": {
                        "low": 500,
                        "high": 1000
                    },
                    "high": {
                        "low": 1000,
                        "high": 1500
                    }
                }
            }
        },
        "timestamp_date_range": {
            "type": "date_range",
            "options": {
                "field": "timestamp",
                "ranges": {
                    "year_before_last": {
                        "start": "2020-01-01T00:00:00Z",
                        "end": "2021-01-01T00:00:00Z"
                    },
                    "last_year": {
                        "start": "2021-01-01T00:00:00Z",
                        "end": "2022-01-01T00:00:00Z"
                    },
                    "this_year": {
                        "start": "2022-01-01T00:00:00Z",
                        "end": "2023-01-01T00:00:00Z"
                    }
                }
            }
        },
        "sum_id": {
            "type": "sum",
            "options": {
                "field": "id"
            }
        },
        "min_id": {
            "type": "min",
            "options": {
                "field": "id"
            }
        },
        "max_id": {
            "type": "max",
            "options": {
                "field": "id"
            }
        },
        "avg_id": {
            "type": "avg",
            "options": {
                "field": "id"
            }
        }
    }
}
