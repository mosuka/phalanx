{
    "query": "search",
    "boost": 1.0,
    "start": 0,
    "num": 5,
    "sort_by": "-_score",
    "fields": [
        "id",
        "title"
    ],
    "aggregations": {
        "text_terms": {
            "type": "terms",
            "options": {
                "field": "text",
                "min_length": 1,
                "max_length": 20,
                "size": 10
            }
        },
        "id_range": {
            "type": "range",
            "options": {
                "field": "id",
                "ranges": {
                    "low": {
                        "from": 0,
                        "to": 500
                    },
                    "medium": {
                        "from": 500,
                        "to": 1000
                    },
                    "high": {
                        "from": 1000,
                        "to": 1500
                    }
                }
            }
        }
    }
}
